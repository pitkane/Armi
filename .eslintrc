{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "react/jsx-no-bind": 0,
    "semi": 0,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "no-unused-vars": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "camelcase": 0,
    "no-console": 0,
    "no-trailing-spaces": 0,
    "no-else-return": 0,
    "max-len": [0, 100, 2, {"ignoreUrls": true}],
    "react/jsx-closing-bracket-location": 0,
  },
  "globals": {
    "$": false
  }
}
